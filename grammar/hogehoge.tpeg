Test1 = { RED &BALL #Test1 }
Test2 = { RED &{BALL} #Test2 }
Test3 = { RED &{BALL} {BALL} #Test3 }
Test4 = { (!BALL .)* #Test4 }
Test5 = { (!{BALL} .)* #Test5 }

RED  = '赤い'
BALL = 'ボール'

example Test1,Test2,Test3,Test4,Test5 赤いボール
// OK Test1 => [#Test1 '赤い']
// OK Test2 => [#Test2 [#Ball 'ボール']]
// OK Test3 => [#Test3 [#Ball 'ボール'] [#Ball 'ボール']]
// OK Test4 => [#Test4 '赤い']
// OK Test5 => [#Test5 '赤い']
